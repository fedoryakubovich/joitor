{
  "name": "joitor",
  "version": "0.2.2",
  "description": "Middleware that helps validate body, headers, cookies, params and query in express application using joi validation.",
  "main": "src/lib/index.js",
  "files": [
    "src/lib",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "joi",
    "express",
    "validator",
    "validation",
    "express-joi"
  ],
  "scripts": {
    "start:server": "nodemon src/tests/server",
    "test": "nyc mocha src/tests/*.test.js --exit",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "lint": "eslint ./src/",
    "lint:fix": "eslint --fix ./src/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fedoryakubovich/joitor.git"
  },
  "author": {
    "name": "Fedor Yakubovich",
    "email": "f.yakubovich@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fedoryakubovich/joitor/issues"
  },
  "homepage": "https://github.com/fedoryakubovich/joitor#readme",
  "nyc": {
    "include": [
      "src/lib/**/*.js"
    ],
    "exclude": [
      "src/tests/**/*.js"
    ]
  },
  "devDependencies": {
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.4",
    "coveralls": "^3.0.9",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-mocha": "^6.3.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.2",
    "express": "^4.17.1",
    "faker": "^4.1.0",
    "mocha": "^7.1.0",
    "nodemon": "^2.0.2",
    "nyc": "^15.0.0",
    "prettier": "^1.19.1",
    "supertest": "^4.0.2"
  },
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "lodash.isempty": "^4.4.0"
  }
}
